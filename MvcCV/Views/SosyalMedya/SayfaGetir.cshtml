@model MvcCV.Models.Entity.TblSosyalMedya

@{
    ViewBag.Title = "SayfaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Sosyal Medya Düzenleme Sayfası</h4>
<br />
<form class="form-group" method="post">

    @Html.Label("Sosyal Medya Adı")
    @Html.TextBoxFor(X => X.Ad, new { @class = "form-control" }) <br />

    @Html.Label("Sosyal Medya Linki")
    @Html.TextBoxFor(X => X.Link, new { @class = "form-control" }) <br />

    @Html.Label("İcon")
    @Html.TextBoxFor(X => X.icon, new { @class = "form-control" }) <br />

    <button class="btn btn-success">Güncelle</button>

</form>

